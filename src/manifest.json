{
    "manifest_version": 2,
    "name": "NSOA",
    "version": "0.0.1",
    "description": "Adds a custom XML editor to Chrome devtools.",
    "icons": {
        "128": "128.png"
    },
    // "background": {
    //     "page": "background.html"
    // },
    // "content_scripts": [
    //     {
    //     "matches": ["https://*.openair.com/*", "https://qa.openair1.com/*"],
    //     "js": ["content.js"]
    //     }
    // ],
    "devtools_page": "devtools.html",
    "options_page": "nsoa_console.html",
    "permissions": [
        "tabs",
        "contextMenus",
        "*://*/*"
    ],
    "web_accessible_resources": [
        "ace/src/*",
        "ace/src/snippets*",
        "store/*",
        "css/*",
        "fonts/*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}